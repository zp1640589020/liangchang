<script type="text/javascript">
		var oBox=document.getElementById('box');
		var oZ=document.getElementById('z');
		var oY=document.getElementById('y');
		var oUl=document.getElementById('box').getElementsByTagName('ul')[0];
		var oLi=document.getElementById('box').getElementsByTagName('li');
		var liL=oLi.length;
		oUl.appendChild(oLi[0].cloneNode(true))
		var liW=fcs(oLi[0],'width');
		var coon=0;
		var timer;
		var lock=true;
		function funn(){
			coon++;
				animate(oUl,{'left':-coon*liW},1000,function(){
					if(coon>liL-1){
						coon=0;
						this.style.left=0+"px";
					}
				})
		}
		oZ.onclick=function(){
			if(oUl.lock){return};
			coon--;
			if(coon<0){
				coon=liL-1;
				oZ.style=-liW*liL+"px";
			}
			animate(oUl,{'left':-liW*coon},1000,function(){
			
			})
	
		}
		oY.onclick=function(){
			if(oUl.lock){return};	
			funn();
		}
		timer=setInterval(function(){
			funn();
		},2000)
		oBox.onmouseover=function(){
			clearInterval(timer);
		}
		oBox.onmouseout=function(){
			timer=setInterval(function(){
			funn();
		},2000)
		}
</script>
